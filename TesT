-- Scripts:



local function XIZSXK_fake_script() -- TextButton.LocalScript 

	local script = Instance.new('LocalScript', TextButton)


	script.Parent.MouseButton1Click:Connect(function()

		script.Parent.Parent.Parent.credits.Visible = false

		script.Parent.Parent.Parent.script.Visible = true

	end)

	

	

end

coroutine.wrap(XIZSXK_fake_script)()

local function KJZCEI_fake_script() -- credsbutton.LocalScript 

	local script = Instance.new('LocalScript', credsbutton)



	script.Parent.MouseButton1Click:Connect(function()

		script.Parent.Parent.Parent.credits.Visible = true

		script.Parent.Parent.Parent.script.Visible = false

	end)

	

	

end

coroutine.wrap(KJZCEI_fake_script)()

local function KNVJAW_fake_script() -- ScreenGui.LocalScript 

	local script = Instance.new('LocalScript', ScreenGui)



	Frame = script.Parent.Frame

	Frame.Draggable = true

	Frame.Active = true

	Frame.Selectable = true

	

end

coroutine.wrap(KNVJAW_fake_script)()

local function UOUP_fake_script() -- ImageButton.LocalScript 

	local script = Instance.new('LocalScript', ImageButton)



	local button = script.Parent

	

	button.MouseButton1Click:Connect(function()

		if button.Parent.Frame.Visible == false then

			button.Parent.Frame.Visible = true

		else

			button.Parent.Frame.Visible = false

		end

	end)

end

coroutine.wrap(UOUP_fake_script)()
